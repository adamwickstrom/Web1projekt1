*{
	margin: 0px;
	padding: 0px;
	box-sizing: border-box;
}

nav{
	display: flex;
	justify-content: space-around;
	align-items: center;
	min-height: 8vh;
	background-color:#528fcc;
	
}

.nav-links{
	display: flex;
	justify-content: space-around;
	width: 50%
}

.nav-links a{
	text-decoration: none;
	letter-spacing: 2px;
	font-size: 16px;
	color: #000000;
}

.nav-links li {
	list-style: none;
}
